{
  "PROJECT_DIR": "~/Repos/libcups",
  "DEPENDENCY_DIR": "~/Repos/libusb",
  "DEPLIB_NAME": "libusb-1.0.a",
  "CUSTOM_HEADERS": {
    "linux_usbfs.c": [
      "libusb/libusbi.h"
    ]
  },
  "EXCLUDE_REGEXES": [
    "Xcode/.*",
    "msvc/.*",
    "examples/.*",
    "tests/.*",
    "libusb/os/darwin*",
    "libusb/os/windows*",
    "libusb/os/haiku*",
    "libusb/os/freebsd*",
    "libusb/os/netbsd*",
    "libusb/os/openbsd*",
    "libusb/os/sunos*",
    "libusb/os/threads_windows.h"
  ],
  "FORCE_RECOMPILE": true,
  "SHOW_FUNCTIONS": false,
  "CBMC_OPTS_STR": "--object-bits 12 --unwind 1",
  "CBMC_TIMEOUT": 7,
  "ENABLE_RESULT_LOG": true,
  "DIE_ON_ERROR": true,
  "SILENT_IDENTITY_VERIFICATION": false,
  "USE_EXISTING_DRIVERS": false,
  "REVERSE_MAPPING": true,
  "VERBOSITY": 2,
  "NPROC": 12,
  "SKIP_BLAME": false,
  "SKIP_IMPACT": true,
  "FULL": true,
  "QUIET_BUILD": true
}


{
  "PROJECT_DIR": "~/Repos/airspy",

  "DEPENDENCY_DIR": "~/Repos/libusb",
  "COMMIT_OLD": "b0fd4d87",
  "COMMIT_NEW": "6cae9c6d",











  "ENABLE_CBMC": true,
  "CBMC_OPTS_STR": "--object-bits 12 --unwind 1 --unwinding-assertions --havoc-undefined-functions",
  "CBMC_TIMEOUT": 7,
  "ENABLE_STATE_SPACE_ANALYSIS": true,







  "CUSTOM_HEADERS": {
    "linux_usbfs.c": [
      "libusb/libusbi.h"
    ],
    "linux_netlink.c": [
      "libusb/libusbi.h"
    ],
    "linux_udev.c": [
      "libusb/libusbi.h"
    ]
  },
  "EXTRA_COMPILE_FLAGS": [
    "-DFD_CLOEXEC",
    "-DHAVE_PTHREAD_SETNAME_NP",
    "-DHAVE_OS_TIMER",
    "-DHAVE_EVENTFD",
    "-DHAVE_LIBUDEV",
    "-DHAVE_TIMERFD",
    "-D_inline=inline",
    "-D__GNUC__",
    "-Wno-missing-prototypes",
    "-Wno-macro-redefined"
  ],
  "EXCLUDE_REGEXES": [
    "android/.*",
    "Xcode/.*",
    "msvc/.*",
    "examples/.*",
    "tests/.*",
    "libusb/os/darwin.*",
    "libusb/os/.*windows.*",
    "libusb/os/haiku.*",
    "libusb/os/freebsd.*",
    "libusb/os/netbsd.*",
    "libusb/os/openbsd.*",
    "libusb/os/sunos.*",
    "libusb/os/threads_windows.h"
  ],
  
  "USE_EXISTING_DRIVERS": false,
  "FORCE_RECOMPILE": false,
  "VERBOSITY": 1,
  "DEPLIB_NAME": "libusb-1.0.a",
  "PAUSES": true,
  "ORDER_BY_CALL_SITE": false
}

{
  "PROJECT_DIR": "~/Repos/airspy",
  "DEPENDENCY_DIR": "~/Repos/libusb",
  "DEPLIB_NAME": "libusb-1.0.a",
  "CUSTOM_HEADERS": {
    "linux_usbfs.c": [
      "libusb/libusbi.h"
    ],
    "linux_netlink.c": [
      "libusb/libusbi.h"
    ],
    "linux_udev.c": [
      "libusb/libusbi.h"
    ]
  },
  "EXTRA_COMPILE_FLAGS": [
    "-DHAVE_TIMERFD",
    "-D_inline=inline",
    "-D__GNUC__",
    "-Wno-missing-prototypes",
    "-Wno-macro-redefined"
  ],
  "EXCLUDE_REGEXES": [
    "android/.*",
    "Xcode/.*",
    "msvc/.*",
    "examples/.*",
    "tests/.*",
    "libusb/os/darwin.*",
    "libusb/os/.*windows.*",
    "libusb/os/haiku.*",
    "libusb/os/freebsd.*",
    "libusb/os/netbsd.*",
    "libusb/os/openbsd.*",
    "libusb/os/sunos.*",
    "libusb/os/threads_windows.h"
  ],
  "FORCE_RECOMPILE": false,
  "FORCE_CCDB_RECOMPILE": false,
  "SHOW_FUNCTIONS": false,
  "CBMC_OPTS_STR": "--object-bits 12 --unwind 1 --unwinding-assertions --havoc-undefined-functions",
  "CBMC_TIMEOUT": 7,
  "ENABLE_RESULT_LOG": true,
  "DIE_ON_ERROR": false,
  "SILENT_IDENTITY_VERIFICATION": true,
  "SILENT_VERIFICATION": true,
  "USE_EXISTING_DRIVERS": false,
  "ORDER_BY_CALL_SITE": true,
  "VERBOSITY": 1,
  
  "SKIP_BLAME": false,
  "SKIP_IMPACT": false,
  "FULL": true,
  "QUIET_BUILD": true
}


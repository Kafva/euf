{
	"PROJECT_DIR": "~/Repos/jabberd-2.7.0",
	"DEPENDENCY_DIR": "~/Repos/libexpat",
	"DEP_SOURCE_ROOT": "~/Repos/libexpat/expat",
	"DEPLIB_NAME": "libexpat.a",

  "DEP_INCLUDE_PATHS": [
    "expat/lib"
  ],
	
  "EXCLUDE_REGEXES": [
    "expat/tests/.*",
    "expat/examples/.*"
  ],
  
  "CUSTOM_HEADERS": {
    "xmlparse.c": [
      "~/Repos/euf/expat/include/xmlparse.h"
    ]
  },

  "CBMC_OPTS_STR": "--object-bits 12 --unwind 1", 
  "CBMC_TIMEOUT": 7,

  "ENABLE_RESULT_LOG": true,
  "DIE_ON_ERROR": true,
  "SILENT_IDENTITY_VERIFICATION": true,
  "SHOW_FUNCTIONS": false,

	"REVERSE_MAPPING": true,
	"VERBOSITY": 2,
	"NPROC": 12,
	"SKIP_BLAME": false,
	"SKIP_IMPACT": true,
	"FULL": true
}

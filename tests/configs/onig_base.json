{
  "PROJECT_DIR": "~/Repos/jq",
  "DEPENDENCY_DIR": "~/Repos/oniguruma",
  "DEPLIB_NAME": "libonig.a",
  "EXCLUDE_REGEXES": [
    "sample/.*",
    "test/.*"
  ],

  "FORCE_RECOMPILE": false,
  "SHOW_FUNCTIONS": false,
  "CBMC_OPTS_STR": "--object-bits 12 --unwind 1",
  "CBMC_TIMEOUT": 7,
  "ENABLE_RESULT_LOG": true,
  "DIE_ON_ERROR": true,
  "SILENT_IDENTITY_VERIFICATION": false,
  "USE_EXISTING_DRIVERS": false,
  "REVERSE_MAPPING": true,
  "VERBOSITY": 2,
  "NPROC": 12,
  "SKIP_BLAME": false,
  "SKIP_IMPACT": true,
  "FULL": true,
  "QUIET_BUILD": true
}


